{
  "name": "@roninjin10/ts-sol",
  "description": "A typesafe library for writing forge scripts in typescript",
  "version": "0.0.1",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "src/index.ts",
  "type": "module",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/roninjin10/stax.git",
    "directory": "packages/babel-plugin-sol"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "envSchema": "ts-node src/scripts/envSchema.ts",
    "build": "tsup",
    "//test": "vitest",
    "//test:ui": "vitest --ui",
    "//test:run": "vitest run",
    "lint": "eslint \"**/*.{ts,tsx}\" --fix && prettier \"src/**/*.{ts,tsx}\" --write",
    "lint:check": "eslint \"**/*.{ts,tsx}\" && prettier --check \"src/**/*.{ts,tsx}\""
  },
  "peerDependencies": {},
  "devDependencies": {
    "@vitest/ui": "^0.28.4",
    "tsup": "^6.6.0",
    "typescript": "^4.9.5",
    "vitest": "^0.28.4",
    "zod": "^3.20.6"
  },
  "dependencies": {
    "@babel/helper-plugin-utils": "^7.20.2",
    "@types/babel-core": "^6.25.7"
  },
  "keywords": [
    "solidity",
    "forge",
    "foundry",
    "sol",
    "typescript",
    "web3",
    "blockchain"
  ]
}
