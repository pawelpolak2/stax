{
  "name": "@roninjin10/typesafe-growthbook",
  "description": "A light wrapper around growthbook based on zod",
  "version": "0.0.1",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "src/index.ts",
  "license": "MIT",
  "private": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "envSchema": "ts-node src/scripts/envSchema.ts",
    "build": "tsup",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "lint": "eslint \"**/*.{ts,tsx}\" --fix && prettier \"src/**/*.{ts,tsx}\" --write",
    "lint:ci": "eslint \"**/*.{ts,tsx}\" && prettier --check \"src/**/*.{ts,tsx}\""
  },
  "peerDependencies": {
    "@growthbook/growthbook-react": "^0.10.0-0",
    "react": "^16.8.0-0 || ^17.0.0-0 || ^18.0.0-0",
    "zod": "*"
  },
  "devDependencies": {
    "@growthbook/growthbook-react": "^0.10.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@vitejs/plugin-react": "^2.2.0",
    "@vitest/ui": "^0.26.3",
    "jest-dom": "link:@types/@testing-library/jest-dom",
    "jsdom": "^21.0.0",
    "matchers": "link:@types/@testing-library/jest-dom/matchers",
    "nodemon": "^2.0.20",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "vite": "^3.2.5",
    "vitest": "^0.26.3",
    "zod": "^3.20.2"
  },
  "dependencies": {}
}
