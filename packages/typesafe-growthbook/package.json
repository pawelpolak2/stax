{
  "name": "@roninjin10/typesafe-growthbook",
  "description": "A light wrapper around growthbook based on zod",
  "version": "0.0.1",
  "main": "dist/roninjin10-typesafe-growthbook.cjs.js",
  "module": "dist/roninjin10-typesafe-growthbook.esm.js",
  "license": "MIT",
  "private": false,
  "files": [
    "dist"
  ],
  "preconstruct": {
    "entrypoints": [
      "TypesafeGrowthbook.ts"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "envSchema": "ts-node src/scripts/envSchema.ts",
    "build": "preconstruct build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "postinstall": "preconstruct dev",
    "lint": "eslint \"**/*.{ts,tsx}\" --fix && prettier \"src/**/*.{ts,tsx}\" --write",
    "lint:ci": "eslint \"**/*.{ts,tsx}\" && prettier --check \"src/**/*.{ts,tsx}\""
  },
  "peerdependencies": {
    "zod": "*",
    "react": "^16.8.0-0 || ^17.0.0-0 || ^18.0.0-0",
    "@growthbook/growthbook-react": "^0.10.0-0"
  },
  "devDependencies": {
    "@preconstruct/cli": "^2.3.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@vitest/ui": "^0.26.3",
    "nodemon": "^2.0.20",
    "@growthbook/growthbook-react": "^0.10.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.9.4",
    "vite": "^3.2.5",
    "vitest": "^0.26.3",
    "zod": "^3.20.2"
  },
  "dependencies": {}
}
