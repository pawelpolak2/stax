{
  "name": "@roninjin10/example-server",
  "version": "0.0.2",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "src/index.ts",
  "license": "MIT",
  "engines": {
    "node": ">=18",
    "pnpm": ">=7"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "envSchema": "tsx src/scripts/envSchema.ts",
    "kill": "npx kill-port 7300",
    "build": "tsup",
    "dev": "tsx watch scripts/run.ts",
    "serve": "node dist/run.ts",
    "test": "echo 'No tests implemented'",
    "lint": "eslint \"**/*.{ts,tsx}\" --fix && prettier \"src/**/*.{ts,tsx}\" --write",
    "lint:ci": "eslint \"**/*.{ts,tsx}\" && prettier --check \"src/**/*.{ts,tsx}\""
  },
  "dependencies": {
    "@prisma/client": "^4.8.1",
    "@roninjin10/typesafe-env": "workspace:*",
    "@trpc/server": "^10.8.1",
    "fastify": "^4.11.0",
    "fastify-metrics": "^10.0.2",
    "prisma": "^4.8.1",
    "superjson": "^1.12.1",
    "trpc-playground": "^1.0.4",
    "tsx": "^3.12.2",
    "typescript": "^4.9.4",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "tsup": "^6.5.0",
    "vitest": "^0.26.3"
  }
}
